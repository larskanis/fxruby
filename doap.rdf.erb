      </doap:Version>
    </doap:release>

    <!-- current fox16 release -->
    <doap:release>
      <doap:Version>
        <doap:branch>fox16</doap:branch>
        <doap:created><%= Date.today %></doap:created>
        <doap:revision><%= Fox.fxrubyversion %></doap:revision>
      </doap:Version>
    </doap:release>

  </doap:Project>
